{
  "name": "maikfigura.de",
  "private": true,
  "description": "My personal website",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md,css}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "test": "jest",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,json,md,css}\"",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ignore-path .gitignore . --ext .js,jsx",
    "validate": "npm-run-all --parallel check-format lint"
  },
  "dependencies": {
    "gatsby": "^2.25.1",
    "gatsby-plugin-postcss": "^3.0.4",
    "gatsby-plugin-react-helmet": "^3.3.14",
    "gatsby-source-filesystem": "^2.4.0",
    "gatsby-transformer-asciidoc": "^1.3.15",
    "highlight.js": "^10.3.2",
    "install": "^0.13.0",
    "npm": "^6.14.8",
    "postcss-import": "^13.0.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "typeface-fira-code": "^1.1.13",
    "typeface-inter": "^1.1.13"
  },
  "devDependencies": {
    "@testing-library/cypress": "^7.0.1",
    "@testing-library/react": "^11.1.0",
    "@types/jest": "^26.0.15",
    "babel-jest": "^26.6.2",
    "babel-preset-gatsby": "^0.5.15",
    "cypress": "^5.5.0",
    "cypress-axe": "^0.10.0",
    "husky": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.2",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.11.5",
    "tailwindcss": "^1.9.6"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maiksensi/maikfigura.de"
  },
  "bugs": {
    "url": "https://github.com/maiksensi/maikfigura.de/issues"
  }
}
