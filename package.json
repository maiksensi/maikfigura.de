{
  "name": "maikfigura.de",
  "private": true,
  "description": "My personal website",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "test:e2e": "start-server-and-test develop http://localhost:8000 cy:open",
    "test:e2e:ci": "start-server-and-test develop http://localhost:8000 cy:run",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "prettier": "prettier --ignore-path .gitignore \"**/*.{js,jsx,json,md}\"",
    "check-format": "npm run prettier -- --list-different",
    "lint": "eslint --ignore-path .gitignore . --ext .js,jsx",
    "validate": "npm-run-all --parallel check-format lint"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.13",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/styles": "^4.9.13",
    "gatsby": "^2.21.13",
    "gatsby-plugin-material-ui": "^2.1.8",
    "gatsby-plugin-react-helmet": "^3.3.1",
    "gatsby-source-filesystem": "^2.3.1",
    "gatsby-transformer-asciidoc": "^1.3.1",
    "highlight.js": "^10.0.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.0.0",
    "typeface-inter": "^3.12.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "devDependencies": {
    "@testing-library/cypress": "^6.0.0",
    "cypress": "^4.5.0",
    "cypress-axe": "^0.8.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.5",
    "start-server-and-test": "^1.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maiksensi/maikfigura.de"
  },
  "bugs": {
    "url": "https://github.com/maiksensi/maikfigura.de/issues"
  }
}
